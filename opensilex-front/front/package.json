    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/typescript"
        ],
        "rules": {
            "no-unused-vars": "off"
        },
        "parserOptions": {
            "parser": "@typescript-eslint/parser"
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
